.overlay
  position: fixed
  top: 0
  bottom: 0
  left: 0
  right: 0
  z-index: 99
  background-color: rgba(0,0,0,0.4)
  display: none
  cursor: pointer
.on
  display: block

.slides
  width: 100%
  overflow: hidden
  position: relative
  height: auto
  min-height: 350px
  .slides2
    position: relative
    overflow: hidden
    width: 2000em
    top: 0
  .slide
    max-width: 960px
    width: auto
    position: relative
    overflow: hidden
    position: relative
    padding: 0
    margin: 0
    float: left
    img
      display: block
      height: auto
      border: 0
  .navigator
    position: relative
    height: 70px
    background: black
    z-index: 99
    width: 100%
  .nav.next
    left: auto
    right: 0
    text-align: right
  .nav.prev
    right: auto
    left: 0
    text-align: left
  .nav
    position: absolute
    top: 0
    bottom: 0
    overflow: hidden
    text-decoration: none
    background: black
    cursor: pointer
    color: white
    width: 100px
    padding: 10px 30px
    &:hover
