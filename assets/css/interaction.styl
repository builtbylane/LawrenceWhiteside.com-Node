@media only screen and (max-width: 40.063em)
  .modal
    height: 100vh
    min-height: 100vh  
@media only screen and (min-width: 40.063em)
  .modal
    left: 50%  
dialog .column,
dialog .columns, .modal .column,
.modal .columns
  min-width: 0 
dialog > :first-child, .modal > :first-child
  margin-top: 0 
dialog > :last-child, .modal > :last-child
  margin-bottom: 0 
@media only screen and (min-width: 40.063em)
  .modal
    margin-left: -40%
    width: 80%  
.modal .close-modal
  font-size: 2.5rem
  line-height: 1
  position: absolute
  top: 0.5rem
  right: 0.6875rem
  color: #aaaaaa
  font-weight: bold
  cursor: pointer 

@media only screen and (min-width: 40.063em)
