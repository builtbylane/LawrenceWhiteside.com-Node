doctype 5
html(lang='en')
  head
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, user-scalable=no')
    title= title
    != css('foundation-icons')
    != css('foundation')
    != css('ionicon.min')
    != css('site')
    != css('interaction')
    != css('nav')
    != css('web')
  body.web('data-video' = 'off')
    block content
    != js('lib/jquery-1.9.1')    
    != js('lib/can.custom')
    != js('lib/modernizr')    
    != js('lib/fastclick')
    != js('lib/TweenMax')
    != js('lib/video.dev')
    != js('lib/iscroll-probe')    
    != js('controls/app')
    != js('controls/presenter')
    != js('controls/nav')
    != js('controls/iphone')
    != js('controls/section')
    != js('controls/bg')
